            "\\.(jpg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
        },
        "transform": {
            "^.+\\.(js|jsx)$": "babel-jest",
            ".+\\.(css|style|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
